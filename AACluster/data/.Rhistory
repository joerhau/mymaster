test.hclust <- function (d, ...) {
  hc <- hclust(dist(d))
  remplir <- function (m, i, res=NULL) {
    if(i<0) {
      return( c(res, -i) )
    } else {
      return( c(res, remplir(m, m[i,1], NULL), remplir(m, m[i,2], NULL) ) )
    }
  }
  a <- remplir(hc$merge, hc$merge[n-1,1])
  b <- remplir(hc$merge, hc$merge[n-1,2])
  co <- rep(1,n)
  co[b] <- 2
  plot(d, col=co, main="hclust", ...)
}
